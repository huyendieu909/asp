@model bai4._2.Models.TienDien
@{
    ViewBag.Title = "TinhTien";
}
<style>
    td {
        border: solid black 1px;
    }
</style>
<h2>Tiền điện mà chủ hộ phải trả</h2>
<div>
    <table>
        <tr>
            <td width="150px"><label>Chủ hộ:</label></td>
            <td width="200px">@Model.hoten</td>
        </tr>
        <tr>
            <td><label>Số điện tiêu thụ:</label></td>
            <td>@Model.dien</td>
        </tr>
        <tr>
            <td><label>Tiền phải trả: </label></td>
            <td>@Model.tien</td>
        </tr>
    </table>
</div>

